<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.rssclient"
       name="RSS Client Background"
       version="1.0.1"
       provider-name="pieh">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="xbmcgui" />
    <import addon="script.rss.editor" />
    <import addon="plugin.video.youtube" />
    <import addon="script.module.rssengine" version="1.0.0" />
  </requires>
  <extension point="xbmc.python.module"
             library="default.py" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">RSS Client Background</summary>
    <description lang="en">For skinners to attach RSS feeds into the skins</description>
    <platform>all</platform>
  </extension>
</addon>
